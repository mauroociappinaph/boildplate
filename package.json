{
  "name": "boildplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "prepare": "husky install",
    "audit:deps": "snyk test",
    "audit:sonar": "sonar-scanner",
    "audit:lighthouse": "lighthouse",
    "audit:deps-cruise": "depcruise",
    "audit:all": "npm run audit:deps && npm run audit:sonar && npm run audit:lighthouse && npm run audit:deps-cruise",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "type-check": "tsc --noEmit",
    "lighthouse": "lighthouse http://localhost:3000 --output json --output html",
    "sonar": "sonar-scanner"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/mixpanel-browser": "^2.60.0",
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "autoprefixer": "10.4.17",
    "concurrently": "^9.1.2",
    "dependency-cruiser": "^15.0.0",
    "eslint": "^9.27.0",
    "eslint-config-next": "^15.3.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lighthouse": "^11.3.0",
    "lint-staged": "^16.0.0",
    "node-cron": "^3.0.3",
    "postcss": "8.4.35",
    "prettier": "^3.5.3",
    "prisma": "^6.8.2",
    "sonarqube-scanner": "^2.8.1",
    "tailwindcss": "3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "turbo": "^1.12.4",
    "typescript": "^5.8.3"
  },
  "packageManager": "npm@10.2.4",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@sentry/nextjs": "^7.101.1",
    "@sentry/node": "^7.101.1",
    "@shadcn/ui": "^0.0.4",
    "@types/axios": "^0.9.36",
    "axios": "^1.9.0",
    "mixpanel-browser": "^2.48.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  }
}
